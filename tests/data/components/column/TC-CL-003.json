{
  "version": "1.0",
  "name": "Column组件间距属性测试",
  "designReference": {
    "width": 1200,
    "height": 800
  },
  "root": "screen",
  "components": {
    "screen": {
      "type": "screen",
      "style": "screenStyle",
      "children": ["container"]
    },
    "container": {
      "type": "container",
      "style": "containerStyle",
      "children": ["title", "columnNoSpacing", "columnSmallSpacing", "columnMediumSpacing", "columnLargeSpacing"]
    },
    "title": {
      "type": "text",
      "style": "titleStyle",
      "text": "Column组件间距属性测试"
    },
    "columnNoSpacing": {
      "type": "column",
      "style": "columnNoSpacingStyle",
      "layout": { "spacing": 0 },
      "children": ["noSpacingTitle", "noSpacingBox1", "noSpacingBox2", "noSpacingBox3"]
    },
    "noSpacingTitle": {
      "type": "text",
      "style": "noSpacingTitleStyle",
      "text": "间距：0px"
    },
    "noSpacingBox1": { "type": "container", "style": "noSpacingBox1Style" },
    "noSpacingBox2": { "type": "container", "style": "noSpacingBox2Style" },
    "noSpacingBox3": { "type": "container", "style": "noSpacingBox3Style" },
    "columnSmallSpacing": {
      "type": "column",
      "style": "columnSmallSpacingStyle",
      "layout": { "spacing": 8 },
      "children": ["smallSpacingTitle", "smallSpacingBox1", "smallSpacingBox2", "smallSpacingBox3"]
    },
    "smallSpacingTitle": {
      "type": "text",
      "style": "smallSpacingTitleStyle",
      "text": "间距：8px"
    },
    "smallSpacingBox1": { "type": "container", "style": "smallSpacingBox1Style" },
    "smallSpacingBox2": { "type": "container", "style": "smallSpacingBox2Style" },
    "smallSpacingBox3": { "type": "container", "style": "smallSpacingBox3Style" },
    "columnMediumSpacing": {
      "type": "column",
      "style": "columnMediumSpacingStyle",
      "layout": { "spacing": 16 },
      "children": ["mediumSpacingTitle", "mediumSpacingBox1", "mediumSpacingBox2", "mediumSpacingBox3"]
    },
    "mediumSpacingTitle": {
      "type": "text",
      "style": "mediumSpacingTitleStyle",
      "text": "间距：16px"
    },
    "mediumSpacingBox1": { "type": "container", "style": "mediumSpacingBox1Style" },
    "mediumSpacingBox2": { "type": "container", "style": "mediumSpacingBox2Style" },
    "mediumSpacingBox3": { "type": "container", "style": "mediumSpacingBox3Style" },
    "columnLargeSpacing": {
      "type": "column",
      "style": "columnLargeSpacingStyle",
      "layout": { "spacing": 32 },
      "children": ["largeSpacingTitle", "largeSpacingBox1", "largeSpacingBox2", "largeSpacingBox3"]
    },
    "largeSpacingTitle": {
      "type": "text",
      "style": "largeSpacingTitleStyle",
      "text": "间距：32px"
    },
    "largeSpacingBox1": { "type": "container", "style": "largeSpacingBox1Style" },
    "largeSpacingBox2": { "type": "container", "style": "largeSpacingBox2Style" },
    "largeSpacingBox3": { "type": "container", "style": "largeSpacingBox3Style" }
  },
  "styles": {
    "screenStyle": { "backgroundColor": "#f5f5f5" },
    "containerStyle": { "width": "100%", "height": "100%", "padding": "20px" },
    "titleStyle": { "fontSize": "24px", "fontWeight": "bold", "marginBottom": "20px" },
    "columnNoSpacingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "16px", "marginBottom": "20px" },
    "noSpacingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "noSpacingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "noSpacingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "noSpacingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnSmallSpacingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "16px", "marginBottom": "20px" },
    "smallSpacingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "smallSpacingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "smallSpacingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "smallSpacingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnMediumSpacingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "16px", "marginBottom": "20px" },
    "mediumSpacingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "mediumSpacingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "mediumSpacingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "mediumSpacingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnLargeSpacingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "16px", "marginBottom": "20px" },
    "largeSpacingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "largeSpacingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "largeSpacingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "largeSpacingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" }
  }
} 