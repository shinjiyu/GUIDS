{
  "version": "1.0",
  "name": "Column组件内边距属性测试",
  "designReference": {
    "width": 1200,
    "height": 800
  },
  "root": "screen",
  "components": {
    "screen": {
      "type": "screen",
      "style": "screenStyle",
      "children": ["container"]
    },
    "container": {
      "type": "container",
      "style": "containerStyle",
      "children": ["title", "columnNoPadding", "columnSmallPadding", "columnMediumPadding", "columnLargePadding"]
    },
    "title": {
      "type": "text",
      "style": "titleStyle",
      "text": "Column组件内边距属性测试"
    },
    "columnNoPadding": {
      "type": "column",
      "style": "columnNoPaddingStyle",
      "layout": { "spacing": 8 },
      "children": ["noPaddingTitle", "noPaddingBox1", "noPaddingBox2", "noPaddingBox3"]
    },
    "noPaddingTitle": {
      "type": "text",
      "style": "noPaddingTitleStyle",
      "text": "内边距：0px"
    },
    "noPaddingBox1": { "type": "container", "style": "noPaddingBox1Style" },
    "noPaddingBox2": { "type": "container", "style": "noPaddingBox2Style" },
    "noPaddingBox3": { "type": "container", "style": "noPaddingBox3Style" },
    "columnSmallPadding": {
      "type": "column",
      "style": "columnSmallPaddingStyle",
      "layout": { "spacing": 8 },
      "children": ["smallPaddingTitle", "smallPaddingBox1", "smallPaddingBox2", "smallPaddingBox3"]
    },
    "smallPaddingTitle": {
      "type": "text",
      "style": "smallPaddingTitleStyle",
      "text": "内边距：8px"
    },
    "smallPaddingBox1": { "type": "container", "style": "smallPaddingBox1Style" },
    "smallPaddingBox2": { "type": "container", "style": "smallPaddingBox2Style" },
    "smallPaddingBox3": { "type": "container", "style": "smallPaddingBox3Style" },
    "columnMediumPadding": {
      "type": "column",
      "style": "columnMediumPaddingStyle",
      "layout": { "spacing": 8 },
      "children": ["mediumPaddingTitle", "mediumPaddingBox1", "mediumPaddingBox2", "mediumPaddingBox3"]
    },
    "mediumPaddingTitle": {
      "type": "text",
      "style": "mediumPaddingTitleStyle",
      "text": "内边距：16px"
    },
    "mediumPaddingBox1": { "type": "container", "style": "mediumPaddingBox1Style" },
    "mediumPaddingBox2": { "type": "container", "style": "mediumPaddingBox2Style" },
    "mediumPaddingBox3": { "type": "container", "style": "mediumPaddingBox3Style" },
    "columnLargePadding": {
      "type": "column",
      "style": "columnLargePaddingStyle",
      "layout": { "spacing": 8 },
      "children": ["largePaddingTitle", "largePaddingBox1", "largePaddingBox2", "largePaddingBox3"]
    },
    "largePaddingTitle": {
      "type": "text",
      "style": "largePaddingTitleStyle",
      "text": "内边距：32px"
    },
    "largePaddingBox1": { "type": "container", "style": "largePaddingBox1Style" },
    "largePaddingBox2": { "type": "container", "style": "largePaddingBox2Style" },
    "largePaddingBox3": { "type": "container", "style": "largePaddingBox3Style" }
  },
  "styles": {
    "screenStyle": { "backgroundColor": "#f5f5f5" },
    "containerStyle": { "width": "100%", "height": "100%", "padding": "20px" },
    "titleStyle": { "fontSize": "24px", "fontWeight": "bold", "marginBottom": "20px" },
    "columnNoPaddingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "0px", "marginBottom": "20px" },
    "noPaddingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "noPaddingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "noPaddingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "noPaddingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnSmallPaddingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "8px", "marginBottom": "20px" },
    "smallPaddingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "smallPaddingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "smallPaddingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "smallPaddingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnMediumPaddingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "16px", "marginBottom": "20px" },
    "mediumPaddingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "mediumPaddingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "mediumPaddingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "mediumPaddingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" },
    "columnLargePaddingStyle": { "width": "300px", "height": "200px", "backgroundColor": "#ffffff", "border": "1px solid #e0e0e0", "borderRadius": "8px", "padding": "32px" },
    "largePaddingTitleStyle": { "fontSize": "16px", "fontWeight": "bold", "marginBottom": "10px" },
    "largePaddingBox1Style": { "width": "100%", "height": "40px", "backgroundColor": "#e3f2fd", "borderRadius": "4px" },
    "largePaddingBox2Style": { "width": "100%", "height": "40px", "backgroundColor": "#e8f5e9", "borderRadius": "4px" },
    "largePaddingBox3Style": { "width": "100%", "height": "40px", "backgroundColor": "#fff3e0", "borderRadius": "4px" }
  }
} 